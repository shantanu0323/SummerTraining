<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="shantanu.summertraining">

    <uses-permission android:name="android.permission.CALL_PHONE"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="shantanu.summertraining.MainActivity"/>

                <category android:name="android.intent.category.DEFAULT"/>
            </intent-filter>
        </activity>
        <activity
            android:name=".SquareApp"
            android:label="SquareApp">
        </activity>
        <activity
            android:name=".HomeActivity"
            android:label="Summer Training">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity
            android:name=".AdvanceCalc"
            android:label="AdvanceCalc">
        </activity>
        <activity
            android:name=".ResultActivity"
            android:label="ResultActivity">
        </activity>
        <activity
            android:name=".SimpleInterest"
            android:label="SimpleInterest">
        </activity>
        <activity android:name=".ImplicitIntent">
        </activity>
        <activity
            android:name=".EmailSend"
            android:label="Email">
        </activity>
        <activity android:name=".ActivityContainingFragment">
        </activity>
        <activity android:name=".DynamicActivity">
        </activity>
    </application>

</manifest>